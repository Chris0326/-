<body>
    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">
        <div class="container d-flex align-items-center justify-content-between">
            <a class="logo d-flex align-items-center me-auto me-lg-0" asp-area="" asp-controller="Home" asp-action="Index">
                <h1>膳糧<span>.</span></h1>
            </a>

            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <nav id="navbar" class="navbar">
                <ul>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Store" asp-action="List">餐廳一覽</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Store" asp-action="Reconnend">人氣餐廳</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Store" asp-action="Latest">新進餐廳</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Blogger" asp-action="BloggerList">膳糧專欄</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Login">登入</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="User" asp-action="Mypage">我的首頁</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Advertisement" asp-action="BuyAdv">廣告行銷方案</a>
                    </li>
                    <li>
                        <a class="nav-link text-dark" asp-area="" asp-controller="Shopping" asp-action="Menu">訂餐</a>
                    </li>
                </ul>
            </nav>

            <div>
                <input type="text" class="textbox-book-a-table" id="txtKeyword" placeholder="搜尋餐廳">
                <a class="button-book-a-table" href="#book-a-table">搜尋</a>
                <a href="@Url.Action("CartView", "Shopping")" >
                    <img src="~/images/Menu/購物車.png" style="width:30px;" />
                </a>

            </div>



            
            <i class="mobile-nav-toggle mobile-nav-show bi bi-list"></i>
            <i class="mobile-nav-toggle mobile-nav-hide d-none bi bi-x"></i>

        </div>
    </header><!-- End Header -->
    @RenderBody()


    <!-- ======= Footer ======= -->
    <footer id="footer" class="footer">

        <div class="container">
            <div class="row gy-3">
                <div class="col-lg-3 col-md-6 d-flex">
                    <i class="bi bi-geo-alt icon"></i>
                    <div>
                        <h4>地址</h4>
                        <p>
                            台北市復興南路一段390號2樓<br>
                            (大安捷運站4號、6號出口)
                        </p>
                    </div>

                </div>

                <div class="col-lg-3 col-md-6 footer-links d-flex">
                    <i class="bi bi-telephone icon"></i>
                    <div>
                        <h4>聯絡方式</h4>
                        <p>
                            <strong>Phone:</strong> (02) 6631-6588(台北窗口)<br>
                            <strong>Email:</strong> iservice@ispan.com.tw<br>
                        </p>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 footer-links d-flex">
                    <i class="bi bi-clock icon"></i>
                    <div>
                        <h4>營業時間</h4>
                        <p>
                            <strong>Mon-Sat:</strong> 9AM - 21PM<br>
                            <strong>Sunday:</strong> Closed
                        </p>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 footer-links">
                    <h4>追蹤我們</h4>
                    <div class="social-links d-flex">
                        <a href="#" class="twitter"><i class="bi bi-twitter"></i></a>
                        <a href="#" class="facebook"><i class="bi bi-facebook"></i></a>
                        <a href="#" class="instagram"><i class="bi bi-instagram"></i></a>
                        <a href="#" class="linkedin"><i class="bi bi-linkedin"></i></a>
                    </div>
                </div>

            </div>
        </div>

        <div class="container">
            <div class="copyright">
                &copy; 2023 <strong><span>ShanLiang</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/yummy-bootstrap-restaurant-website-template/ -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
            </div>
        </div>

    </footer><!-- End Footer -->

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

