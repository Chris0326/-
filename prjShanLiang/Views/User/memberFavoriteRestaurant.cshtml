@model IEnumerable<prjShanLiang.Models.MemberAction>

@{
    ViewData["Title"] = "我的收藏";
}

<main id="main">
    
        <!-- ======= Breadcrumbs ======= -->
    <div class="breadcrumbs" style="background-color:orange;">
            <div class="container">
                <div class="d-flex justify-content-between align-items-center">
                    <h2>@ViewData["Title"]</h2>
                </div>
            </div>
        </div><!-- End Breadcrumbs -->
    <div class="container aos-init " data-aos="fade-up">
<table class="table">
    <thead>
        <tr>           
            <th>
                餐廳名稱
            </th>
            <th>
                我的收藏
            </th>
            <th>

            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Store.RestaurantName
            </td>
            <td>
               @item.Action.ActionName
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.No">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.No">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.No">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
    </div>
</main><!-- End #main -->